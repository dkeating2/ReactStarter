{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint2": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:format": "",
    "lint:code": "",
    "test": "yarn test:vitest && yarn test:cypress:component && yarn test:cypress:e2e",
    "test:vitest": "vitest --run --coverage",
    "test:cypress:component": "cypress run --component",
    "ci:e2e": "concurrently \"cypress run\" --kill-others \"yarn preview\" --success first ",
    "test:cypress:e2e": "cypress run --e2e",
    "test:merge-coverage": "npx istanbul-merge --out ./test/merged/coverage/coverage.json ./test/vitest/coverage/coverage-final.json ./test/cypress/coverage/coverage-final.json && npx istanbul report --include ./test/merged/coverage/coverage.json --dir ./test/merged/coverage html",
    "knip": "knip"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.10.0",
    "concurrently": "^8.2.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.12",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-sonar": "^0.12.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "knip": "^3.3.2",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "typescript": "^5.2.2",
    "vite": "4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "nyc": {
    "report-dir": "test/cypress/coverage"
  }
}
